<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Controller">
   <children>
      <BorderPane fx:id="borderpane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1000.0" prefWidth="1000.0">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="openIcon" mnemonicParsing="false" onAction="#openFile" text="Open" />
                        <MenuItem fx:id="saveIcon" mnemonicParsing="false" onAction="#saveFile" text="Save" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Control">
                  <items>
                    <MenuItem fx:id="startIcon" mnemonicParsing="false" onAction="#fightStart" text="Start" />
                        <MenuItem fx:id="CBformation" mnemonicParsing="false" onAction="#GformationSet" text="GOODFormation" />
                        <MenuItem fx:id="MONformation" mnemonicParsing="false" onAction="#BformationSet" text="BADFormation" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem fx:id="infoIcon" mnemonicParsing="false" onAction="#showInfo" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <center>
            <AnchorPane fx:id="anchorpane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</Group>
